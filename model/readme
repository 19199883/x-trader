build:
1. module:gfortran -ffree-line-length-none  -J ./obj -c ./src/MySub_Fuc.f90 -fPIC  -o MySub_Fuc.o
2. c++: g++ -c -shared  -std=c++11 ./src/timestamp.cpp -fPIC  -o timestamp.o

3. gfortran -fPIC  ./obj/Hi5_Func.o ./obj/Common_Func.o ./obj/Hi5_Var_Def.o ./obj/Common_Mod.o ./obj/MySub_Fuc.o ./obj/Common_Var_Def.o ./obj/time.o -shared -Wl,--exclude-libs,ALL -o bin/st_test.sod

